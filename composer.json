{
  "name": "plaisio/canonical-hostname-resolver-static",
  "description": "PhpPlaisio: Canonical Hostname Resolver with static hostname",
  "keywords": [
    "PhpPlaisio",
    "Canonical",
    "Hostname",
    "Resolver",
    "Static"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.3",
    "plaisio/canonical-hostname-resolver": "^1.3.0"
  },
  "require-dev": {
    "phing/phing": "^3.0.1",
    "phpunit/phpunit": "^11.5.22"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\CanonicalHostnameResolver\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\CanonicalHostnameResolver\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
